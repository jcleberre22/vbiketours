CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE SALLE(IDT INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NOM VARCHAR(35),BATIMENT VARCHAR(35),FICPLAN VARCHAR(20))
CREATE MEMORY TABLE EMPLACEMENT(IDT INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,IDTSALLE INTEGER,LOCALISATION CHAR(4),NUMPRISE VARCHAR(8),XDEB INTEGER,YDEB INTEGER,LARGEUR INTEGER,HAUTEUR INTEGER)
CREATE MEMORY TABLE UC(IDT INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CODE CHAR(7),ADIP VARCHAR(19),ETAT CHAR(7))
CREATE MEMORY TABLE POSTE(IDT INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,IDTEMPLACEMENT INTEGER,IDTUC INTEGER,IDTPERS INTEGER)
CREATE MEMORY TABLE PERSONNEL(IDT INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NOM VARCHAR(35),PRENOM VARCHAR(25),GRADE CHAR(7),AFFECTATION VARCHAR(15),TEL VARCHAR(17),STAGE VARCHAR(15))
ALTER TABLE SALLE ALTER COLUMN IDT RESTART WITH 5
ALTER TABLE EMPLACEMENT ALTER COLUMN IDT RESTART WITH 25
ALTER TABLE UC ALTER COLUMN IDT RESTART WITH 25
ALTER TABLE POSTE ALTER COLUMN IDT RESTART WITH 25
ALTER TABLE PERSONNEL ALTER COLUMN IDT RESTART WITH 18
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
