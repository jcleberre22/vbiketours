CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE EQUIPAGE(IDEQUIPAGE INTEGER NOT NULL PRIMARY KEY,LIBELLEEQUIPAGE VARCHAR(30) NOT NULL)
CREATE MEMORY TABLE ESCADRON(IDESCADRON INTEGER NOT NULL PRIMARY KEY,LIBELLEESCADRON VARCHAR(100) NOT NULL,LIEU VARCHAR(15) NOT NULL)
CREATE MEMORY TABLE ARMEMENT(IDARMEMENT INTEGER NOT NULL PRIMARY KEY,LIBELLEARMEMENT VARCHAR(30) NOT NULL)
CREATE MEMORY TABLE CIRCULATION(IDCIRCULATION INTEGER NOT NULL PRIMARY KEY,LIBELLECIRCULATION VARCHAR(30) NOT NULL)
CREATE MEMORY TABLE CATEGORIE(IDCATEGORIE INTEGER NOT NULL PRIMARY KEY,LIBELLECATEGORIE VARCHAR(30) NOT NULL)
CREATE MEMORY TABLE MISSION(IDMISSION INTEGER NOT NULL PRIMARY KEY,TYPEMISSION VARCHAR(30) NOT NULL)
CREATE MEMORY TABLE EVENEMENT(IDEVENEMENT INTEGER NOT NULL PRIMARY KEY,LIBELLEEVENEMENT VARCHAR(30) NOT NULL)
CREATE MEMORY TABLE AVION(IDAVION INTEGER NOT NULL PRIMARY KEY,IMMATRICULATION VARCHAR(15) NOT NULL,NOMAVION VARCHAR(20),TYPEAVION VARCHAR(20) NOT NULL,ARMEAVION INTEGER NOT NULL,HEURESDEVOL INTEGER NOT NULL,INDISPONIBILITE BOOLEAN NOT NULL,TRANSFERT BOOLEAN NOT NULL,CONSTRAINT FK_IDARMEMENT FOREIGN KEY(ARMEAVION) REFERENCES ARMEMENT(IDARMEMENT),CONSTRAINT CHK_HDV CHECK(AVION.HEURESDEVOL>=0))
CREATE MEMORY TABLE QUALIFICATION(IDQUALIFICATION INTEGER NOT NULL PRIMARY KEY,LIBELLEQUALIFICATION VARCHAR(50) NOT NULL)
CREATE MEMORY TABLE PILOTE(IDPILOTE INTEGER NOT NULL PRIMARY KEY,NIA VARCHAR(15) NOT NULL,NID VARCHAR(10) NOT NULL,GRADE VARCHAR(3) NOT NULL,NOMPILOTE VARCHAR(30) NOT NULL,PRENOMPILOTE VARCHAR(50) NOT NULL,QUALIFICATION INTEGER NOT NULL,EQUIPAGE INTEGER,ESCADRON INTEGER,ROLE VARCHAR(15) NOT NULL,NBSORTIESAERIENNES INTEGER NOT NULL,NBHEURESDEVOL INTEGER NOT NULL,EXAMENDEVOL BOOLEAN NOT NULL,CONSTRAINT FK_ESCADRON FOREIGN KEY(ESCADRON) REFERENCES ESCADRON(IDESCADRON),CONSTRAINT FK_EQUIPAGE FOREIGN KEY(EQUIPAGE) REFERENCES EQUIPAGE(IDEQUIPAGE),CONSTRAINT FK_QUALIF FOREIGN KEY(QUALIFICATION) REFERENCES QUALIFICATION(IDQUALIFICATION),CONSTRAINT CHK_NBSORTIES CHECK(PILOTE.NBSORTIESAERIENNES>=0),CONSTRAINT CHK_NBHVOL CHECK(PILOTE.NBHEURESDEVOL>=0))
CREATE MEMORY TABLE SORTIEAERIENNE(IDSORTIE INTEGER NOT NULL PRIMARY KEY,AVION INTEGER NOT NULL,ARMEMENT INTEGER NOT NULL,EQUIPAGE INTEGER NOT NULL,MISSION INTEGER NOT NULL,DATEDECOLLAGE TIMESTAMP NOT NULL,DATEATTERRISSAGE TIMESTAMP NOT NULL,TYPEEVENEMENT INTEGER,EVENEMENT VARCHAR(100),DEBRIEFING VARCHAR(150),CONSTRAINT FK_ARM FOREIGN KEY(ARMEMENT) REFERENCES ARMEMENT(IDARMEMENT),CONSTRAINT FK_AVION FOREIGN KEY(AVION) REFERENCES AVION(IDAVION),CONSTRAINT FK_EQUIPAGE_SORTIE FOREIGN KEY(EQUIPAGE) REFERENCES EQUIPAGE(IDEQUIPAGE),CONSTRAINT FK_TYPE_EVENT FOREIGN KEY(TYPEEVENEMENT) REFERENCES EVENEMENT(IDEVENEMENT),CONSTRAINT FK_MISSION FOREIGN KEY(MISSION) REFERENCES MISSION(IDMISSION))
CREATE MEMORY TABLE VOL(IDVOL INTEGER NOT NULL PRIMARY KEY,CIRCULATION INTEGER NOT NULL,CATEGORIE INTEGER NOT NULL,DATEDECOLLAGE TIMESTAMP NOT NULL,DATEATTERRISSAGE TIMESTAMP NOT NULL,SORTIE INTEGER,ANNULATION BOOLEAN NOT NULL,CONSTRAINT FK_CATEG FOREIGN KEY(CATEGORIE) REFERENCES CATEGORIE(IDCATEGORIE),CONSTRAINT FK_CIRCU FOREIGN KEY(CIRCULATION) REFERENCES CIRCULATION(IDCIRCULATION),CONSTRAINT FK_SORTIE FOREIGN KEY(SORTIE) REFERENCES SORTIEAERIENNE(IDSORTIE))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
